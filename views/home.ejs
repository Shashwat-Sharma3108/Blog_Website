<%- include("partials/header")%>
    <h1>Home</h1>
    <p> <%= homeStratingContent %>  </p>
    
    <% content.forEach((element) => { %>
        <h1> <%= element.title %>  </h1>
        <p> <!--style="width:500px; white-space: nowrap; text-overflow: ellipsis; overflow: hidden;"-->
            <%= element.content.substring(0,100) +  "..." %>
            <a href= "/posts/<%=element.title%>"> Read More</a>
        </p>
    <% }); %>
    
